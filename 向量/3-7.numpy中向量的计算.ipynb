   "source": [
    "import dis\n",
    "import numpy as np\n",
    "\n",
    "a=np.array([3,2,1])\n",
    "b=np.array([1,2,3])\n",
    "# 向量加\n",
    "display(a+b)\n",
    "# 向量乘以常量\n",
    "display(a*3)\n",
    "# 向量单纯的乘法\n",
    "display(a*b)\n",
    "# 点乘\n",
    "display(a.dot(b))\n",
    "# 取模\n",
    "display(np.linalg.norm(a))\n",
    "# 单位向量\n",
    "display(a/np.linalg.norm(a))"
   ]
